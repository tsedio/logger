{
  "name": "@tsed/logger",
  "version": "6.2.0",
  "description": "A multi channel logger written in TypeScript.",
  "author": {
    "name": "Romain Lenzotti"
  },
  "license": "MIT",
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn barrels && yarn run build:esm && yarn run build:cjs",
    "build:cjs": "tsc --build tsconfig.compile.json",
    "build:esm": "tsc --build tsconfig.compile.esm.json",
    "barrels": "barrelsby --delete -d ./src -e \"\\.spec\\.ts\" -e \"__mock__\" -e \".benchmark.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsedio/logger.git"
  },
  "keywords": [
    "ts",
    "log",
    "debug",
    "typescript",
    "trace"
  ],
  "bugs": {
    "url": "https://github.com/tsedio/logger/issues"
  },
  "homepage": "https://github.com/tsedio/logger",
  "dependencies": {
    "colors": "1.4.0",
    "date-format": "^4.0.6",
    "semver": "^7.3.5",
    "tslib": "2.3.1"
  },
  "devDependencies": {},
  "peerDependencies": {}
}